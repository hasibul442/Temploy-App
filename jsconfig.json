{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-native",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "noEmit": true,
    "strict": false,
    "resolveJsonModule": true
  },
  "include": ["**/*.js", "**/*.jsx"],
  "exclude": ["**/node_modules", "**/Pods", "android", "ios"]
}